<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="articlePopulate" class="com.blog.converter.populators.impl.DefaultArticlePopulate"/>

    <bean id="articleConverter" class="com.blog.converter.Converter">
        <property name="targetClass" value="com.blog.datas.ArticleData"/>
        <property name="populate" ref="articlePopulate"/>
    </bean>

</beans>